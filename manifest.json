{
	"manifest_version": 2,
	"name": "Example",
	"version": "0.0.1",
	"description": "Block sites",
	"icons": {
		"48":"assets/buster-icon.png",
		"128": "assets/buster-icon.png"
	},
	"homepage_url": "https://github.com/zuripabon/chrome-block-site",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"128": "assets/buster-icon.png"
		}
	},
	"permissions": [
		"webRequestBlocking",
		"webRequest",
		"activeTab",
		"tabs",
		"http://*/*",
		"https://*/*"
	 ],
	"web_accessible_resources": ["assets/*"]
}
